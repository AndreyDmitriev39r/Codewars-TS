{
  "compilerOptions": {    
    "target": "ES2020",
    "module": "ESNext",

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,

    "outDir": "compiled_solutions"    
  },
  "include": ["solutions"]
}